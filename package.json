{
  "name": "skillforge-ai",
  "version": "1.1.0",
  "description": "SkillForge AI - The AI Skills Platform integrating ClawdBot, Kimi K2.5, Kilo, and Craft Agents with CI Intelligence",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "node --test",
    "start": "node dist/server.js",
    "start:dev": "node dist/index.js",
    "skill:install": "node dist/cli/skill-installer.js",
    "skill:list": "node dist/cli/skill-lister.js",
    "bridge:start": "node dist/mcp-bridge/server.js",
    "ci:analyze": "node dist/ci-intelligence/run-analysis.js",
    "ci:analyze:competitor": "node dist/ci-intelligence/run-analysis.js --competitor",
    "ci:report": "node dist/ci-intelligence/generate-report.js",
    "ci:full": "npm run ci:analyze && npm run ci:report"
  },
  "keywords": [
    "clawdbot",
    "moltbot",
    "kimi",
    "kilo",
    "craft-agents",
    "mcp",
    "ai-assistant",
    "local-first",
    "privacy",
    "competitive-intelligence",
    "video-analysis",
    "nvidia-nim"
  ],
  "author": "Ariel Shapira / Manus AI",
  "license": "Apache-2.0",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.19",
    "@modelcontextprotocol/sdk": "^1.24.3",
    "@supabase/supabase-js": "^2.45.0",
    "axios": "^1.7.0",
    "ws": "^8.18.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.8",
    "@types/ws": "^8.5.12",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/breverdbidder/skillforge-ai.git"
  }
}
